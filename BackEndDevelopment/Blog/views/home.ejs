<%- include("partials/header"); -%>
<h1>Home</h1>
<p> <%= homePageContent %> </p>
<% posts.forEach(function(post){ %>
  <h2> <%= post.title  %></h2>
  <% str="/posts/"+post.title.replace(" ","-"); %>
  <p> <%= post.post.slice(0,100)+"..." %><a href=<%= str %>>Read more</a></p>
<% }); %>
<%- include("partials/footer"); -%>
